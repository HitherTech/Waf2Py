/*Bulma css colors*/

/*Light colors*/
.success_light_bulma { 
  background-color: #ECF9F1;
  color: #1c6e3c;
}



.grey_light_bulma { 
  background-color: #f3f3f3;
  color: #4c4c4c;
}
 
.info_light_bulma { 
  background-color: #eaf5fb;
  color: #2680ad;
}

.primary_light_bulma {
  background-color: #e7fffc;
  color: #0d8f7b;
}

.warning_light_bulma {
  background-color: #fffae9;
  color: #927020;
}
.link_light_bulma {
  background-color: #ebf1fb;
  color: #0059b9;
}

.danger_light_bulma {
  background-color: #ffe9ee;
  color: #cc0533;
}

/*Solid colors*/
.success_bulma { 
  background-color: #37bf6d;
  color: #1c6e3c;
}
.success_white_bulma { 
  background-color: #37bf6d;
  color: #ffffff;
}
.primary_bulma { 
  background-color: #00cba9;
  color: #ffffff;
}
.info_bulma { 
  background-color: #008fd4;
  color: #ffffff;
}
.link_bulma { 
  background-color: #006bd3;
  color: #ffffff;
}
.warning_bulma { 
  background-color: #ffd558;
  color: #50431c;
}
.danger_bulma { 
  background-color: #f33a5f ;
