{{extend 'layout_community.html'}}
<title> Error Application Logs</title>


<div class="row">
    <div class="col-md-12">
        <div class="box table-responsive ">
                <div class="box-body">
                <table id="datatable_error_app" class="table table-striped table-bordered">
                    <thead>
                    <tr>
                        <th>Time</th>
                        <th>Username</th>
                        <th>Description</th>
                    </tr>
                    </thead>

                    <tbody>
                      {{ for row in query: }}
                    <tr>
                      <td> {{=row.time}} </td>
                      <td> {{=row.username}}</td>
                      <td> {{=row.msg}}</td>

                    </tr>
                        {{pass}}
                    </tbody>
                </table>
                </div>

        </div>
    </div>

</div> <!-- End Row -->

<script>
    $(document).ready(function (){
    $("#datatable_error_app").dataTable(
        {
            mark: true
        }
    )
    });
</script>
